import random
arr = []

for i in range(0, 6):
    rnd = int((random.random()*45+1)-1)
    arr.append(rnd)

print(arr)

arr1 =[1,2,3,4,5,6,7]

for idx, i in arr1:
    rnd= int((random.random()*45+1)-1)
    arr1[idx] = rnd
    
arr2=[1,2,3,4]
for i in range(10):
    rnd = int(random.random()*4)
    a = arr[0]
    b = arr[rnd]
    arr[0] =b
    arr[rnd] = a
