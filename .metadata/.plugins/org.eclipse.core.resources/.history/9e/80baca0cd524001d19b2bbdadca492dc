import random

mine = input("안내면 진다 가위/바위/보>>")
com = ""
result = ""

rnd = int((random.random()*3)+1)

if rnd == 1:
    com="가위"
elif rnd ==2:
    com = "바위"
else:
    com = "보"
if mine == com:
    result="비김"
if mine == "가위" and com =="바위" & mine == "바위" and com =="보":
    result = "짐"
     
    
