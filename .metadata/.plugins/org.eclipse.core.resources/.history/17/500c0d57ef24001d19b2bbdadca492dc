from day02.myfor04 import cnt
class LeeJY:
    def __init__(self):
        self.money = 10
    def phonepari(self):
        self.money += 5

class Byden:
    def __init__(self):
        self.cnt_war = 1
    def bubkyu(self,cnt):
        self.cnt_war += cnt

class HyungJ(LeeJY, Byden):
    def __init__(self):
        super(LeeJY,self).__init__()
        super(Byden,self).__init__()

HJ = HyungJ()
        

