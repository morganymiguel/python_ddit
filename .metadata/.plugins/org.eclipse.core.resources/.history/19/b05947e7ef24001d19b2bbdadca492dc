from day02.myfor04 import cnt
class LeeJY:
    def __init__(self):
        self.money = 10
    def phonepari(self):
        self.money += 5

class Byden:
    def __init__(self):
        self.cnt_war = 1
    def bubkyu(self,cnt):
        self.cnt_war += cnt

class HyungJ(LeeJY, Byden):
    def __init__(self):
        super().__init__()

HJ = HyungJ()
print(HJ.bubkyu(3))
print(HJ.phonepari(2000))
print(HJ.cnt_war,HJ.money)

